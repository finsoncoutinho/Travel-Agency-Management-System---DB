//TABLE:consumer
create table consumer(Firstname varchar2(20), Lastname varchar2(20),Phonenumber varchar2(20),Email varchar2(20) primary key,Password varchar2(20), gender varchar2(5));
Select * from consumer;

//TABLE:admin
create table admin(Email varchar(20), Password varchar(20)); 
Select * from admin;

//TABLE:booking
create table booking(Email varchar(20) primary key, travel_id int, Qseats int, Price int, TotalPrice int); 
Select * from booking;
